		"docs": "parcel src/_docs/*.html --dist-dir docs  --public-url /prolin/"
	},
	"@parcel/transformer-css": {
		"drafts": {
			"nesting": true
		}
	},
	"devDependencies": {
		"parcel": "^2.10.3"
	}
}
