{
	"name": "prolin",
	"version": "1.0.0",
	"description": "A Class-Less CSS Library",
	"license": "MIT",
	"author": "Takuto Yanagida",
	"homepage": "https://github.com/takty/prolin#readme",
	"keywords": [
		"css",
		"html",
		"style"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/takty/prolin.git"
	},
	"bugs": {
		"url": "https://github.com/takty/prolin/issues"
	},
	"browserslist": [
